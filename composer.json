{
    "name": "tdashton/openapi-php-lib",
    "description": "A PHP Library to assist in creation of OpenApi Specifications",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.5",
        "phpunit/php-code-coverage": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "Finderly\\OpenapiPhpLib\\": "src/OpenapiPhpLib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Finderly\\OpenapiPhpLib\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tyler Ashton",
            "email": "tdashton@gmail.com"
        }
    ],
    "scripts":{
        "phpunit": "phpunit",
        "phpstan": "phpstan analyze src/"
    }
}
